.icons{
  width: 570rpx;
  height: 60rpx;
  border: 1rpx solid #ccc;
  border-radius: 40rpx;
  margin:20rpx auto;
  padding: 0 10rpx; 
  overflow: hidden;
  white-space: nowrap;
}
.icons_img{
  height: 60rpx;
  width: 60rpx;
  margin: 0 10rpx;
}
.cent{
  position: relative;
}
.port{
  position: absolute;
  width:500rpx;
  height:667rpx;
  top:0;
  left:125rpx;
}
.port .box{
  position: absolute;
  background-size: 150rpx 150rpx;
  top:258.5rpx;
  left:175rpx;
  transform-origin: 50% 50%;
}
.port .box .boxImg{
  width: 150rpx;
  height:150rpx;
  box-sizing: border-box;
}
.port .box.active{
  border: 5rpx dashed #f84 ;
}
.box .moveclose,.box .movescale{
  width:30rpx;
  height:30rpx;
  position: absolute;
}
.box .moveclose{
  bottom:-30rpx;
  left:0;
  transform-origin: 0 0;
  background: url(https://l0vwp10g.qcloud.la/section-close.png?x-oss-process=image/resize,m_lfit,w_100) no-repeat center;
  background-size: 30rpx;
}
.box .movescale{
  transform-origin: 100% 0;
  bottom:-30rpx;
  right:0;
  background: url(https://l0vwp10g.qcloud.la/section-magnify.png?x-oss-process=image/resize,m_lfit,w_100) no-repeat center;
  background-size: 30rpx;
}
.image{
  width:500rpx;
  height:667rpx;
  display: block;
  margin: 0 auto;
}
.btns{
  width: 50rpx;
  height: 200rpx;
  position: absolute;
  top:200rpx;
  right:50rpx;
}
.btns text{
  width: 30rpx;
  height:50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 24rpx;
  padding: 0 10rpx;
  display: block;
  border:1rpx solid #ccc;
  border-radius: 5rpx;
}
.btns text.active{
  background-color: yellow;
}
.footer{
  width:100%;
  position: fixed;
  bottom:0;
}
.sure{
  text-align: center;
  height:100rpx;
  line-height: 100rpx;
  background-color: yellow;
}
.icon view{
  width:100rpx;
  text-align: center;
  padding: 25rpx;
  background-color: #fff;
  border-top: 1rpx solid #eee;
}
.icon image{
  width:50rpx;
  height:50rpx;
}
.prop{
  display: block;
  font-size: 20rpx;
}
.sucai .top{
  background-color: yellow;
  text-align: center;
  font-size: 40rpx;
  padding: 20rpx;
}
.sucai .top .img{
  width:40rpx;
  height:40rpx;
}
.top .close{
  width: 60rpx;
  height:60rpx;
  background: url("https://l0vwp10g.qcloud.la/footer-save.png") no-repeat center;
  background-size: 30rpx;
}
.sucai .nav{
  height:65rpx;
  white-space:nowrap;
  padding-bottom: 10rpx;
  background: #fff;
}
.sucai .nav .navList{
  font-size: 20rpx;
  line-height: 20rpx;
  padding: 0 30rpx;
  padding-bottom: 15rpx;
  display: inline-block
}
.sucai .nav .navList.active{
  border-bottom: 5rpx solid yellow;
}
.list{
  white-space:nowrap;
  height:190rpx;
}
.list .navList{
  display: inline-block;
  padding: 20rpx;
  font-size: 20rpx;
  text-align: center
}
.sucaiIcon{
  width: 100rpx;
  height: 100rpx;
  display: block;
  margin: 10rpx auto;
}
.huizhang{
  color:yellow;
  display: block;
  height: 30rpx;
  border-radius: 15rpx;
  background-color: #449;
}